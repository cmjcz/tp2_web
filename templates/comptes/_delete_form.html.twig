<form method="post" action="{{ path('comptes_delete', {'idcomptes': compte.idcomptes}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ compte.idcomptes) }}">
    <button class="btn">Delete</button>
</form>
